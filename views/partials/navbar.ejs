                    <% if (currentUser){%>
                        <a href="/user/myurls">
                            <div class="px-4 py-3">
                                <span class="block c-transition  text-secondary">
                                    <%= currentUser.username %>
                                </span>
                                <span class="block text-xs c-transition  text-textMutedS truncate ">
                                    <%= currentUser.email %>
                                </span>
                            </div>
                        </a>
                        <% } %>
