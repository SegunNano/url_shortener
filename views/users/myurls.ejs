<% layout('layouts/boilerplate')%>
    <% if (userUrls) { %>
        <% if (userUrls.length) { %>
            <div class="ml-0 p-4 border-2 border-gray-200 border-dashed rounded-lg dark:border-gray-700">
                <% for(url of userUrls) { %>
                    <div class="mt-8 space-y-4">
                        <div
                            class="flex flex-wrap items-center bg-textMuted shadow-[0_2px_6px_-1px_rgba(0,0,0,0.3)] p-2  border-x-0 border-b-0 w-full border border-t-4 border-t-textMutedS rounded-xl ">
                            <div class="ml-4 flex-1 overflow-hidden">
                                <p class="text-md text-secondary font-semibold">
                                    <%= url.originalUrl %>
                                </p>
                                <p class="text-sm text-textMutedS mt-1 md:mt-2">
                                    <%= url.shortenedUrl %>
                                </p>
                            </div>
                            <div
                                class="mx-1 flex flex-col justify-between items-center gap-4 min-h-full text-secondary">
                                <a href="<%=`http://localhost:5000/dev_nano/view/${url._id}/`%> "
                                    class="cursor-pointer">
                                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 16 16" fill="currentColor"
                                        class="size-5">
                                        <path fill-rule="evenodd"
                                            d="M2.75 9a.75.75 0 0 1 .75.75v1.69l2.22-2.22a.75.75 0 0 1 1.06 1.06L4.56 12.5h1.69a.75.75 0 0 1 0 1.5h-3.5a.75.75 0 0 1-.75-.75v-3.5A.75.75 0 0 1 2.75 9ZM2.75 7a.75.75 0 0 0 .75-.75V4.56l2.22 2.22a.75.75 0 0 0 1.06-1.06L4.56 3.5h1.69a.75.75 0 0 0 0-1.5h-3.5a.75.75 0 0 0-.75.75v3.5c0 .414.336.75.75.75ZM13.25 9a.75.75 0 0 0-.75.75v1.69l-2.22-2.22a.75.75 0 1 0-1.06 1.06l2.22 2.22H9.75a.75.75 0 0 0 0 1.5h3.5a.75.75 0 0 0 .75-.75v-3.5a.75.75 0 0 0-.75-.75ZM13.25 7a.75.75 0 0 1-.75-.75V4.56l-2.22 2.22a.75.75 0 1 1-1.06-1.06l2.22-2.22H9.75a.75.75 0 0 1 0-1.5h3.5a.75.75 0 0 1 .75.75v3.5a.75.75 0 0 1-.75.75Z"
                                            clip-rule="evenodd" />
                                    </svg>
                                </a>
                                <form action="/dev_nano/<%= url._id%>?_method=DELETE" method="POST"
                                    class="cursor-pointer">
                                    <button type="submit">
                                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 16 16" fill="currentColor"
                                            class="size-5">
                                            <path fill-rule="evenodd"
                                                d="M5 3.25V4H2.75a.75.75 0 0 0 0 1.5h.3l.815 8.15A1.5 1.5 0 0 0 5.357 15h5.285a1.5 1.5 0 0 0 1.493-1.35l.815-8.15h.3a.75.75 0 0 0 0-1.5H11v-.75A2.25 2.25 0 0 0 8.75 1h-1.5A2.25 2.25 0 0 0 5 3.25Zm2.25-.75a.75.75 0 0 0-.75.75V4h3v-.75a.75.75 0 0 0-.75-.75h-1.5ZM6.05 6a.75.75 0 0 1 .787.713l.275 5.5a.75.75 0 0 1-1.498.075l-.275-5.5A.75.75 0 0 1 6.05 6Zm3.9 0a.75.75 0 0 1 .712.787l-.275 5.5a.75.75 0 0 1-1.498-.075l.275-5.5a.75.75 0 0 1 .786-.711Z"
                                                clip-rule="evenodd" />
                                        </svg>
                                    </button>
                                </form>
                            </div>
                        </div>
                    </div>
                    <% } %>
            </div>
            <%} else { %>
                <div class="p-4 border-2 mt-auto border-gray-200 border-dashed rounded-lg dark:border-gray-700">
                    <div class="mt-8 space-y-4">
                        <div class="w-full card">
                            <p class="p-3 xl:text-start text-lg text-secondary font-semibold text-center">You don't
                                have any
                                shortened
                                links yet.
                            </p>
                        </div>
                    </div>
                </div>
                <% } %>
                    <% } %>